{
  "name": "base-fs-rename",
  "description": "Plugin for 'base' applications that adds a `rename` method that, when called, can be passed to `app.dest()` as the rename function (this is an instance plugin, not pipeline plugin)",
  "version": "0.1.1",
  "homepage": "https://github.com/node-base/base-fs-rename",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-fs-rename",
  "bugs": {
    "url": "https://github.com/node-base/base-fs-rename/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "extend-shallow": "^2.0.1",
    "is-absolute": "^0.2.3"
  },
  "devDependencies": {
    "base": "^0.6.7",
    "base-fs": "^0.3.0",
    "delete": "^0.3.0",
    "gulp-format-md": "^0.1.5",
    "mocha": "*"
  },
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "base",
        "base-fs",
        "assemble-fs",
        "base-task",
        "base-fs-conflicts"
      ]
    },
    "reflinks": [
      "verb",
      "base-fs",
      "assemble-fs"
    ],
    "lint": {
      "reflinks": true
    }
  }
}